#%RAML 1.0
title: Sensor Interface
mediaType: application/json
version: v1
baseUri: localhost:8000/api?token=x
/getdata:
  description: "Get Data"
  get:
    responses:
      200:
        body:
          properties:
            sensor_id:
              type: integer
            sensor_data:
              type: number

/setupsensor:
  description: "Setup"
  post:
    body:
      properties:
        param1:
          type: integer
        param2:
          type: string
